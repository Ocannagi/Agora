						"exec": [
							"if(pm.response.status == \"OK\")\r",
							"{\r",
							"    let jsonData = pm.response.json();\r",
							"    if(jsonData)\r",
							"        pm.environment.set(\"token\", jsonData.jwt);\r",
							"\r",
							"}\r",
							"\r",
							"let jsonData;\r",
							"try{\r",
							"    jsonData = pm.response.json();\r",
							"}\r",
							"catch(e){\r",
							"    jsonData = pm.response.text();\r",
							"}\r",
							"\r",
							"pm.test(`${pm.request.method.toLowerCase()}${pm.request.url.path[2]} - Status code is 200`, function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(`${pm.request.method.toLowerCase()}${pm.request.url.path[2]} - Body is an object`, function () {\r",
							"    pm.expect(jsonData).to.be.an('object');\r",
							"});\r",
							"\r",
							"pm.test(`${pm.request.method.toLowerCase()}${pm.request.url.path[2]} - Body have a property \"jwt\" that is a \"string\"`, function () {\r",
							"    pm.expect(jsonData).to.have.own.property('jwt').that.is.a('string');\r",
							"});\r",
